{
  "name": "mern",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "stackblitz": {
    "startCommand": "npm start"
  },
  "proxy": "https://localhost:3001",
  "scripts": {
    "build-dev": "webpack build --mode development",
    "build-prod": "webpack build --mode production",
    "build-css": "node-sass --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "start-js": "webpack serve",
    "start": "concurrently \"npm run watch-css\" \"npm run start-js\"",
    "build": "npm run build-css && npm run build-prod"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "history": "^5.3.0",
    "jwt-decode": "^4.0.0",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.4",
    "react-select": "^5.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "bootstrap": "^4.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.6.0",
    "jquery": "^3.7.1",
    "node-sass": "^9.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "popper.js": "^1.16.1",
    "postcss-loader": "^8.1.0",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "tether": "^2.0.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
